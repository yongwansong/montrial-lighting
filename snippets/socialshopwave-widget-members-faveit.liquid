{% comment %}
/**************************************************
* DO NOT MODIFY THIS FILE!!!                      *
* IT WILL BE OVERWRITTEN BY UPDATES FROM GROWAVE! *
**************************************************/
{% endcomment %}
{% if template contains 'product' and product %}
<div id="widget-members-faveit-html"></div>
<script type="text/javascript" defer>
  if(typeof sswWidgetMembersFaveInterval == 'undefined'){
    var sswWidgetMembersFaveInterval = setInterval(function(){
      if(typeof sswJqLoaded != 'undefined' && sswJqLoaded){
        clearInterval(sswWidgetMembersFaveInterval);
        var data = {
          'module': 'user',
          'name': 'fave',
          'product_id': {{ product.id }},
          '_sid': sswCookie('hesid')
        };

        ssw.get(sswProxyUrl + '/lite2/core/widget/index', data, function(response){
          ssw('#widget-members-faveit-html').html(response);
        });
      }
    }, 300);
  }
</script>
{% endif %}