    "trade_text_grid_AYmyfP": {
      "type": "trade__text-grid",
      "blocks": {
        "text_ijGHzR": {
          "type": "text",
          "disabled": true,
          "settings": {
            "heading": "Gabrielle Bendayan",
            "subHeading": "@gabrielle_bendayan",
            "detail": "<p>\"Being at Union inspires me to want to create unique and different spaces for my client. They have a great support system which really helps maximize my time with my client. We work hand in hand to get the project done smoothly and efficiently.\"<\/p>"
          }
        },
        "text_Ragkan": {
          "type": "text",
          "disabled": true,
          "settings": {
            "heading": "Beacon Home",
            "subHeading": "Design@beaconhome_design",
            "detail": "<p>\"Union is a designer's dream come true. When we walked in, all of our favorite designers and designs were here. It's a one-stop-shop for all lighting, furniture and hardware pieces.\"<\/p>"
          }
        },
        "text_rHziCJ": {
          "type": "text",
          "disabled": true,
          "settings": {
            "heading": "Natasha K",
            "subHeading": "Design@natasha.k.design",
            "detail": "<p>\"Being at Union inspires me to want to create. They're up to date with the latest styles and trends, they carry so many brands that I adore. When you're coming here with a client, and you want to show multiple products at once, it's a great hub to do so.\"<\/p>"
          }
        }
      },
      "block_order": [
